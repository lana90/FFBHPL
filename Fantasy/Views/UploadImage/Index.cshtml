@{
    ViewBag.Title = "Index";
    //Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Upload Image</h2>

@using (Html.BeginForm(null, null, FormMethod.Post, new { enctype = "multipart/form-data" })) {
    <table>
        <tr>
            <td>
                File:
            </td>
            <td>
                <input type = "file" name = "File" id = "File" />
            </td>
        </tr>
        <tr>
            <td>
                &nbsp;
            </td>
            <td>
                <input type = "submit" name = "submit" value = "Upload" />
            </td>
        </tr>
    </table> 
}